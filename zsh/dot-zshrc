plugins=(
				fzf
				git
				gitignore
				ssh
				ssh-agent
				sudo
				tmux
				web-search
				you-should-use
				zoxide
				zsh-autosuggestions
				zsh-syntax-highlighting
				)

source $ZSH/oh-my-zsh.sh

